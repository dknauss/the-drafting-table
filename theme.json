{
    "$schema": "https://schemas.wp.org/wp/6.6/theme.json",
    "version": 3,
    "settings": {
        "appearanceTools": true,
        "layout": {
            "contentSize": "860px",
            "wideSize": "1280px"
        },
        "color": {
            "palette": [
                {
                    "slug": "parchment",
                    "color": "#f5f0e8",
                    "name": "Parchment"
                },
                {
                    "slug": "parchment-dark",
                    "color": "#ece5d8",
                    "name": "Parchment Dark"
                },
                {
                    "slug": "ink",
                    "color": "#2c2a26",
                    "name": "Ink"
                },
                {
                    "slug": "ink-light",
                    "color": "#5a5650",
                    "name": "Ink Light"
                },
                {
                    "slug": "ink-faint",
                    "color": "#706a64",
                    "name": "Ink Faint"
                },
                {
                    "slug": "ink-ghost",
                    "color": "#d5cfc6",
                    "name": "Ink Ghost"
                },
                {
                    "slug": "terra",
                    "color": "#b5553d",
                    "name": "Terra Cotta"
                },
                {
                    "slug": "terra-muted",
                    "color": "#c47764",
                    "name": "Terra Muted"
                },
                {
                    "slug": "charcoal",
                    "color": "#3a3733",
                    "name": "Charcoal"
                },
                {
                    "slug": "white",
                    "color": "#ffffff",
                    "name": "White"
                }
            ]
        },
        "custom": {
            "borderLight": "rgba(213, 207, 198, 0.5)",
            "borderFrame": "rgba(90, 86, 80, 0.15)",
            "dotGrid": "rgba(140, 130, 115, 0.22)",
            "gridLine": "rgba(160, 150, 135, 0.15)",
            "vellumEdge": "rgba(245, 240, 232, 0.6)",
            "terraTint": "rgba(181, 85, 61, 0.25)",
            "logoFilter": "none"
        },
        "typography": {
            "fontFamilies": [
                {
                    "fontFamily": "'Bodoni Moda', 'Georgia', serif",
                    "slug": "bodoni",
                    "name": "Bodoni Moda",
                    "fontFace": [
                        {
                            "fontFamily": "Bodoni Moda",
                            "fontWeight": "400 700",
                            "fontStyle": "normal",
                            "fontDisplay": "swap",
                            "src": [
                                "file:./fonts/bodoni-moda-v28-latin-ext.woff2",
                                "file:./fonts/bodoni-moda-v28-latin.woff2"
                            ]
                        },
                        {
                            "fontFamily": "Bodoni Moda",
                            "fontWeight": "400",
                            "fontStyle": "italic",
                            "fontDisplay": "swap",
                            "src": [
                                "file:./fonts/bodoni-moda-v28-italic-latin-ext.woff2",
                                "file:./fonts/bodoni-moda-v28-italic-latin.woff2"
                            ]
                        }
                    ]
                },
                {
                    "fontFamily": "'Courier Prime', 'Courier New', monospace",
                    "slug": "courier-prime",
                    "name": "Courier Prime",
                    "fontFace": [
                        {
                            "fontFamily": "Courier Prime",
                            "fontWeight": "400",
                            "fontStyle": "normal",
                            "fontDisplay": "swap",
                            "src": [
                                "file:./fonts/courier-prime-v11-regular-latin-ext.woff2",
                                "file:./fonts/courier-prime-v11-regular-latin.woff2"
                            ]
                        },
                        {
                            "fontFamily": "Courier Prime",
                            "fontWeight": "400",
                            "fontStyle": "italic",
                            "fontDisplay": "swap",
                            "src": [
                                "file:./fonts/courier-prime-v11-italic-latin-ext.woff2",
                                "file:./fonts/courier-prime-v11-italic-latin.woff2"
                            ]
                        },
                        {
                            "fontFamily": "Courier Prime",
                            "fontWeight": "700",
                            "fontStyle": "normal",
                            "fontDisplay": "swap",
                            "src": [
                                "file:./fonts/courier-prime-v11-700-latin-ext.woff2",
                                "file:./fonts/courier-prime-v11-700-latin.woff2"
                            ]
                        }
                    ]
                },
                {
                    "fontFamily": "'Josefin Sans', 'Helvetica Neue', sans-serif",
                    "slug": "josefin",
                    "name": "Josefin Sans",
                    "fontFace": [
                        {
                            "fontFamily": "Josefin Sans",
                            "fontWeight": "300 600",
                            "fontStyle": "normal",
                            "fontDisplay": "swap",
                            "src": [
                                "file:./fonts/josefin-sans-v34-latin-ext.woff2",
                                "file:./fonts/josefin-sans-v34-latin.woff2"
                            ]
                        }
                    ]
                },
                {
                    "fontFamily": "'Caveat', cursive",
                    "slug": "caveat",
                    "name": "Caveat",
                    "fontFace": [
                        {
                            "fontFamily": "Caveat",
                            "fontWeight": "400 700",
                            "fontStyle": "normal",
                            "fontDisplay": "swap",
                            "src": [
                                "file:./fonts/caveat-v23-latin-ext.woff2",
                                "file:./fonts/caveat-v23-latin.woff2"
                            ]
                        }
                    ]
                }
            ],
            "fontSizes": [
                {
                    "slug": "tiny",
                    "size": "0.5625rem",
                    "name": "Tiny"
                },
                {
                    "slug": "small",
                    "size": "0.8125rem",
                    "name": "Small"
                },
                {
                    "slug": "medium",
                    "size": "1rem",
                    "name": "Medium"
                },
                {
                    "slug": "large",
                    "size": "1.25rem",
                    "name": "Large"
                },
                {
                    "slug": "x-large",
                    "size": "1.75rem",
                    "name": "Extra Large"
                },
                {
                    "slug": "xx-large",
                    "size": "clamp(2.25rem, 4vw, 3.5rem)",
                    "name": "Display"
                }
            ]
        },
        "spacing": {
            "units": [
                "px",
                "em",
                "rem",
                "vh",
                "vw",
                "%"
            ],
            "spacingSizes": [
                {
                    "slug": "10",
                    "size": "0.5rem",
                    "name": "Tiny"
                },
                {
                    "slug": "20",
                    "size": "1rem",
                    "name": "Small"
                },
                {
                    "slug": "30",
                    "size": "1.5rem",
                    "name": "Medium"
                },
                {
                    "slug": "40",
                    "size": "2.5rem",
                    "name": "Large"
                },
                {
                    "slug": "50",
                    "size": "4rem",
                    "name": "Extra Large"
                },
                {
                    "slug": "60",
                    "size": "6rem",
                    "name": "Huge"
                }
            ]
        },
        "border": {
            "color": true,
            "radius": true,
            "style": true,
            "width": true
        }
    },
    "styles": {
        "color": {
            "background": "var(--wp--preset--color--parchment)",
            "text": "var(--wp--preset--color--ink)"
        },
        "typography": {
            "fontFamily": "'Courier Prime', 'Courier New', monospace",
            "fontSize": "1.0625rem",
            "lineHeight": "1.65"
        },
        "css": "body {-webkit-font-smoothing: antialiased; -moz-osx-font-smoothing: grayscale;}",
        "elements": {
            "heading": {
                "typography": {
                    "fontFamily": "'Bodoni Moda', 'Georgia', serif",
                    "fontWeight": "400",
                    "lineHeight": "1.15"
                },
                "color": {
                    "text": "var(--wp--preset--color--ink)"
                }
            },
            "h1": {
                "typography": {
                    "fontSize": "clamp(2.25rem, 4vw, 3.5rem)",
                    "letterSpacing": "0.02em"
                }
            },
            "h2": {
                "typography": {
                    "fontFamily": "'Josefin Sans', 'Helvetica Neue', sans-serif",
                    "fontWeight": "600",
                    "fontSize": "0.8125rem",
                    "letterSpacing": "0.35em",
                    "textTransform": "uppercase"
                }
            },
            "h3": {
                "typography": {
                    "fontSize": "1.25rem",
                    "letterSpacing": "0.03em"
                }
            },
            "h4": {
                "typography": {
                    "fontSize": "1rem",
                    "fontFamily": "'Josefin Sans', 'Helvetica Neue', sans-serif",
                    "fontWeight": "300",
                    "letterSpacing": "0.25em",
                    "textTransform": "uppercase"
                }
            },
            "link": {
                "color": {
                    "text": "var(--wp--preset--color--terra)"
                },
                ":hover": {
                    "color": {
                        "text": "var(--wp--preset--color--ink-light)"
                    }
                }
            },
            "button": {
                "typography": {
                    "fontFamily": "'Josefin Sans', 'Helvetica Neue', sans-serif",
                    "fontWeight": "300",
                    "fontSize": "0.6875rem",
                    "letterSpacing": "0.3em",
                    "textTransform": "uppercase"
                },
                "color": {
                    "background": "transparent",
                    "text": "var(--wp--preset--color--ink)"
                },
                "border": {
                    "color": "var(--wp--preset--color--ink-ghost)",
                    "width": "1px",
                    "style": "solid"
                },
                ":hover": {
                    "color": {
                        "background": "var(--wp--preset--color--ink)",
                        "text": "var(--wp--preset--color--parchment)"
                    },
                    "border": {
                        "color": "var(--wp--preset--color--ink)"
                    }
                }
            }
        },
        "blocks": {
            "core/separator": {
                "color": {
                    "text": "var(--wp--preset--color--ink-ghost)"
                },
                "border": {
                    "color": "var(--wp--preset--color--ink-ghost)",
                    "width": "1px"
                }
            },
            "core/quote": {
                "typography": {
                    "fontFamily": "'Bodoni Moda', 'Georgia', serif",
                    "fontStyle": "italic",
                    "fontSize": "1.25rem",
                    "lineHeight": "1.6"
                },
                "color": {
                    "text": "var(--wp--preset--color--ink-light)"
                },
                "border": {
                    "left": {
                        "color": "var(--wp--preset--color--terra)",
                        "width": "2px",
                        "style": "solid"
                    }
                }
            }
        }
    },
    "templateParts": [
        {
            "name": "header",
            "title": "Header",
            "area": "header"
        },
        {
            "name": "footer",
            "title": "Footer",
            "area": "footer"
        }
    ],
    "patterns": [
        "the-drafting-table/hero-cover",
        "the-drafting-table/portfolio-grid",
        "the-drafting-table/services-overview",
        "the-drafting-table/testimonial-panel",
        "the-drafting-table/process-workflow",
        "the-drafting-table/contact-inquiry",
        "the-drafting-table/latest-journal-entries"
    ],
    "customTemplates": [
        {
            "name": "page-no-title",
            "title": "Page (No Title)",
            "postTypes": [
                "page"
            ]
        },
        {
            "name": "page-wide",
            "title": "Page (Wide)",
            "postTypes": [
                "page"
            ]
        },
        {
            "name": "page-journal",
            "title": "Journal",
            "postTypes": [
                "page"
            ]
        },
        {
            "name": "page-projects",
            "title": "Projects",
            "postTypes": [
                "page"
            ]
        },
        {
            "name": "page-principles",
            "title": "Principles",
            "postTypes": [
                "page"
            ]
        },
        {
            "name": "page-about",
            "title": "About",
            "postTypes": [
                "page"
            ]
        }
    ]
}